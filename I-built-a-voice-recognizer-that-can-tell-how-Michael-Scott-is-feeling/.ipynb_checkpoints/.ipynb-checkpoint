    "from sklearn.utils import shuffle\n",
    "\n",
    "def readAudioFiles(direc):\n",
    "    df = pd.DataFrame(columns=['feature'])\n",
    "    i = 0\n",
    "    \n",
    "    for audiofile in os.listdir(direc):\n",
    "        # Load file using librosa\n",
    "        X, sr = librosa.load(audiofile, res_type = 'kaiser_fast', duration = 5, sr = 44100, offset = 0.5)\n",
    "        sr = np.array(sr)\n",
    "        # Extract the MFCCS\n",
    "        mfccs = np.mean(librosa.feature.mfcc(y = X, \n",
    "                                            sr = sr, \n",
    "                                            n_mfcc = 13),\n",
    "                        axis=0)\n",
    "        feature = mfccs\n",
    "        # Add to data frame\n",
    "        df.loc[i] = [feature]\n",
    "        i += 1\n",
    "    df = pd.DataFrame(df['feature'].values.tolist())\n",
    "    df = shuffle(newdf)\n",
    "    df = df.fillna(0)\n",
    "    return df\n",
    "    \n",
    "audio_features = readAudioFiles('the-office-audio-clips/')      "
   ]
  },
